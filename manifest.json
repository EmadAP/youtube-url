{
    "manifest_version": 3,
    "name": "YouTube URL Modifier",
    "version": "1.0",
    "description": "Appends &t=60s to YouTube video URLs after 60 seconds.",
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    }
  }
  